
<div class="rate-table">
    <div class="row">
        <div class="col-md-3 col-sm-6 col-xs-6">
            <div class="rate-counter-block">
                <div class="icon rate-icon  "> <img src="/img/activity.svg"  class="icon-svg-1x"></div>
                <div class="rate-box">
                    <h1 class="loan-rate">{{ data.activityScore}}</h1>
                  <a href="#activity">
                      <small class="rate-title">Activity</small>
                  </a>
                </div>
            </div>
        </div>
        <div class="col-md-3 col-sm-6 col-xs-6">
            <div class="rate-counter-block">
                <div class="icon rate-icon  "> <img src="/img/social.svg"  class="icon-svg-1x"></div>
